global:
  imagePullSecrets: 
    - my-registry-secret

# Idle Culler (Moved to top-level)
cull:
  enabled: true
  timeout: 36000  # 10 hours
  every: 300      # check every 5 minutes
  users: false

# JupyterHub Configuration
jupyterhub:
  proxy:
    secretToken: ce065069680ad88ff24caada1be8d807b8c5cdc61fbdd681f16887d642d32b8d
    service:
      type: LoadBalancer
      annotations:
        cloud.google.com/neg: '{"ingress": true}' 

  hub:
    extraConfig:
      jupyterhub_config.py: |
        c.JupyterHub.hub_connect_ip = 'hub'
        c.JupyterHub.hub_ip = '0.0.0.0'
        c.Spawner.delete_on_logout = False
    db:
      type: postgres
      url: postgresql://usernameCHANGE:passwordCHANGE@jupyter-postgresql:5432/jupyterhub
    config:
      Authenticator:
        admin_users:
          - 'admin'
      JupyterHub:
        authenticator_class: nativeauthenticator.NativeAuthenticator
        cookie_secret: "b4f298f5a7444ba1fc6ac361a7f439bf1562acfbc8a3d8b9b21c599bd0c16127"
        log_level: "DEBUG"
      NativeAuthenticator:
        open_signup: true
        allowed_failed_logins: 5
        seconds_before_next_try: 300

  singleuser:
    startTimeout: 900
    storage:
      type: dynamic
      capacity: 10Gi
      dynamic:
        storageClass: standard
    profileList:
      - display_name: "Pytorch"
        description: "Pytorch environment"
        default: true
        kubespawner_override:
          image: gcr.io/sdss-ching-yao/my-repo/pytorch
          cpu_guarantee: 14
          mem_guarantee: "35G" 
          tolerations:         
            - key: "nvidia.com/gpu"
              operator: "Equal"
              value: "present"
              effect: "NoSchedule" 
          extra_resource_limits:
            nvidia.com/gpu: 1
          extra_resource_guarantees:
            nvidia.com/gpu: 1
        extraConfig:
          dockerd:
            defaultRuntime: nvidia

      - display_name: "Tensorflow 2.18"
        description: "Tensorflow 2.18"
        default: true
        kubespawner_override:
          image: gcr.io/sdss-ching-yao/my-repo/tf-18
          cpu_guarantee: 14
          mem_guarantee: "35G" 
          tolerations:         
            - key: "nvidia.com/gpu"
              operator: "Equal"
              value: "present"
              effect: "NoSchedule" 
          extra_resource_limits:
