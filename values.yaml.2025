  prePuller:
    hook:
      enabled: false

postgresql:
  image:
    tag: latest

  global:
    storageClass: "standard"

  postgresql:
    image:
      tag: "13.3.0-debian-10-r22"

  auth:
    enablePostgresUser: true
    username: "usernameCHANGE"
    password: "passwordCHANGE"
    database: "jupyterhub"

  persistence:
    enabled: true
    size: "2Gi"

  resources:
    requests:
      memory: "512Mi"
      cpu: "300m"
    limits:
      memory: "1Gi"
      cpu: "500m"

  volumePermissions:
    enabled: true

